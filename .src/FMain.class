' Gambas class file

Public Sub hostMinderLauncher_Click()

  Exec ["hostminder"]

End

Public Sub showHideToggle_Click()

  If showHideToggle.Value = False Then
    Shell ("sed -i '/X-GNOME-Autostart-enabled=/d' ~/.config/autostart/ubuntu-ce-welcome.desktop; echo 'X-GNOME-Autostart-enabled=false' >> ~/.config/autostart/ubuntu-ce-welcome.desktop;")
  Else
    Shell ("sed -i '/X-GNOME-Autostart-enabled=/d' ~/.config/autostart/ubuntu-ce-welcome.desktop; echo 'X-GNOME-Autostart-enabled=true' >> ~/.config/autostart/ubuntu-ce-welcome.desktop")
  End If

End

